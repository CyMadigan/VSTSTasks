{
  "manifestVersion": 1,
  "id": "PathGate",
  "name": "Path Filter Gate",
  "version": "0.0.1",
  "publisher": "kerwincarpede",
  "galleryFlags": ["Private"],
  "scopes": ["vso.work", "vso.code", "vso.build_execute"],
  "demands": ["api-version/3.0"],
  "repository": {
    "type": "git",
    "uri": "https://github.com/kerwinc/VSTSTasks/tree/master/Tasks/PathGate"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "A branch gate build task to help you prevent stale branches and avoid rolling back changes",
  "categories": ["Build and release"],
  "tags": ["Gates", "Build", "Rules", "Cancel Build", "Path Filter"],
  "screenshots": [],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "license.txt"
    }
  },
  "links": {
    "home": {
      "uri": "http://devbraino.com"
    },
    "repository": {
      "uri": "https://github.com/kerwinc/VSTSTasks/tree/master/Tasks/PathFilterGate"
    },
    "issues": {
      "uri": "https://github.com/kerwinc/VSTSTasks/issues"
    }
  },
  "branding": {
    "color": "#8e44ad",
    "theme": "dark"
  },
  "icons": {
    "default": "images/extension-icon.png"
  },
  "files": [
    {
      "path": "Task"
    }
  ],
  "contributions": [
    {
      "id": "pathfilter-branch-gate-task",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "Task"
      }
    }
  ]
}
